{
	"semi": false,
	"printWidth": 88,
	"singleQuote": false,
	"bracketSpacing": true,
	"insertPragma": false,
	"requirePragma": false,
	"jsxSingleQuote": false,
	"bracketSameLine": false,
	"embeddedLanguageFormatting": "auto",
	"htmlWhitespaceSensitivity": "css",
	"vueIndentScriptAndStyle": true,
	"quoteProps": "as-needed",
	"proseWrap": "preserve",
	"trailingComma": "es5",
	"arrowParens": "avoid",
	"useTabs": true,
	"tabWidth": 2
}
